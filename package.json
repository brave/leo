    "@storybook/addon-svelte-csf": "4.1.3",
    "@storybook/addon-themes": "8.1.9",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/svelte": "8.1.9",
    "@storybook/svelte-vite": "8.1.9",
    "@tsconfig/svelte": "5.0.4",
    "@types/jest": "29.5.12",
    "babel-loader": "9.1.3",
    "css-loader": "6.11.0",
    "css-tree": "2.3.1",
    "figma-api-exporter": "github:brave/figma-api-exporter#c70ac90",
    "jsdom": "21.1.2",
    "lodash.camelcase": "4.3.0",
    "lodash.merge": "4.6.2",
    "postcss": "8.4.38",
    "postcss-js": "4.0.1",
    "postcss-sort-media-queries": "4.2.1",
    "prettier": "3.1.0",
    "prettier-plugin-svelte": "3.1.2",
    "rollup": "3.15.0",
    "rollup-plugin-svelte": "7.1.6",
    "sass": "1.58.0",
    "sass-loader": "13.2.0",
    "storybook": "8.1.9",
    "style-dictionary": "3.7.2",
    "style-loader": "3.3.4",
    "svelte-check": "3.6.9",
    "svelte-loader": "3.1.9",
    "svelte2tsx": "0.7.9",
    "svg2vectordrawable": "2.9.1",
    "svgo": "3.0.2",
    "ts-jest": "29.0.5"
  },
  "peerDependencies": {
    "react": ">= 16.0.0",
    "typescript": ">= 4.7.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "scripts": {
    "format": "prettier ./ --write",
    "format-diff": "prettier ./ --list-different",
    "prepare": "npm run build",
    "build": "npm run transform-tokens && npm run skiafy-icons && npm run android-icons && rollup -c rollup.config.js",
    "dev": "rollup -c rollup.config.js --watch",
    "update-icons": "node ./src/scripts/update-icons.js",
    "skiafy-icons": "node ./src/scripts/gen-skia-icons.js",
    "android-icons": "node ./src/scripts/gen-android-icons.js",
    "check": "svelte-check --compiler-warnings a11y-click-events-have-key-events:ignore,a11y-no-static-element-interactions:ignore && node ./src/scripts/audit-events.js",
    "transform-tokens": "node ./src/tokens/transformTokens.js && tsc -p ./tsconfig-tokens.json",
    "test": "jest tests --coverage=false",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "bin": {
    "leo-check": "./src/scripts/audit-tokens.js"
  },
  "license": "MIT",
  "overrides": {
    "axios": "1.7.2"
  }
}
